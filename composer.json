{
    "name": "lsurma/laravel-request-logger",
    "description": "Simple request logger package for laravel",
    "type": "library",
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/console": "^6.0",
        "illuminate/http": "^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Surma",
            "email": "kontakt@lsurma.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LSurma\\LaravelRequestLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LSurma\\LaravelRequestLogger\\RequestLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
